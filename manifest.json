{
  "name": "Doggos Everywhere",
  "version": "0.1",
  "description": "Replace every image with doggos.",
  "manifest_version": 2,
  "icons": {
    "16": "assets/images/icon16.png",
    "32": "assets/images/icon32.png",
    "48": "assets/images/icon48.png",
    "128": "assets/images/icon128.png"
  },
  "content_scripts": [{
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js":      ["assets/js/contentScript.js"]
  }],
  "background": {
    "scripts": ["assets/js/background.js"],
    "persistent": false
  },
  "permissions": [
    "https://dog.ceo/*"
  ]
}